{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",  // Ubah dari public/server.js menjadi dist/server.js
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"  // Ubah dari public/server.js menjadi dist/server.js
    }
  ]
}

  
